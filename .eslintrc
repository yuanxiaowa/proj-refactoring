{
  // http://eslint.org/docs/user-guide/configuring#specifying-parser-options
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  // http://eslint.org/docs/user-guide/configuring#specifying-environments
  "env": {
    "browser": true
  },
  // http://eslint.org/docs/user-guide/configuring#specifying-globals
  "globals": {

  },
  // http://eslint.org/docs/rules/
  "rules": {
    "comma-dangle": [2, "never"],
    "no-cond-assign": [1, "except-parens"],
    "no-console": [1, {
      "allow": ["warn", "error", "log"]
    }],
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-debugger": 1,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty": [2, {
      "allowEmptyCatch": true
    }],
    "no-empty-character-class": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 1,
    "no-extra-parens": [1, "all", {
      "nestedBinaryExpressions": false
    }],
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-inner-declarations": [2, "both"],
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": [2, {
      "skipComments": true
    }],
    "no-negated-in-lhs": 1,
    "no-obj-calls": 2,
    "no-regex-spaces": 1,
    "no-sparse-arrays": 1,
    "no-unexpected-multiline": 2,
    "no-unreachable": 2,
    "no-unsafe-finally": 2,
    "use-isnan": 2,
    "valid-jsdoc": [2, {
      "prefer": {
        "return": "returns"
      },
      "requireReturn": false,
      "requireParamDescription": true,
      "requireReturnDescription": true,
      "requireReturnType": true,
      "matchDescription": "^.+$"
    }],
    "valid-typeof": 2,

    "accessor-pairs": 0,
    "array-callback-return": 2,
    // "block-scoped-var:": 2,
    "complexity": [2, 2],
    "consistent-return": 2,
    "curly": 1,
    "default-case": 0,
    "dot-location": [1, "property"],
    "dot-notation": 1,
    "eqeqeq": [1, "smart"],
    "guard-for-in": 2,
    "no-alert": 2,
    "no-caller": 2,
    "no-case-declarations": 2,
    "no-div-regex": 0,
    "no-else-return": 1,
    "no-empty-function": 1,
    "no-empty-pattern": 2,
    "no-eq-null": 0,
    "no-eval": 2,
    "no-extend-native": [1, {
      "exceptions": []
    }],
    "no-extra-bind": 1,
    // "no-extra-label": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 1,
    "no-implicit-coercion": [1, {
      "boolean": true,
      "string": false,
      "number": false,
      "allow": ["!!", "~", "+"]
    }],
    "no-implicit-globals": 1,
    "no-implied-eval": 2,
    "no-invalid-this": 0,
    "no-iterator": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-magic-numbers": [1, {
      "ignoreArrayIndexes": true
    }],
    "no-multi-spaces": 1,
    "no-multi-str": 1,
    "no-native-reassign": [2, {
      "exceptions": []
    }],
    "no-new": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-octal": 2,
    "no-octal-escape": 2,
    "no-param-reassign": [1, {
      "props": false
    }],
    "no-proto": 2,
    "no-redeclare": [2, {
      "builtinGlobals": true
    }],
    "no-return-assign": 1,
    "no-script-url": 2,
    "no-self-assign": 2,
    "no-self-compare": 2,
    "no-sequences": 1,
    "no-throw-literal": 2,
    "no-unmodified-loop-condition": 2,
    "no-unused-expressions": 1,
    "no-unused-labels": 2,
    "no-useless-call": 2,
    "no-useless-concat": 1,
    "no-useless-escape": 2,
    "no-void": 1,
    "no-warning-comments": [1, {
      "terms": ["todo", "fixme", "xxx"],
      "location": "start"
    }],
    "no-with": 2,
    "radix": 1,
    "vars-on-top": 1,
    "wrap-iife": [1, "any"],
    "yoda": [1, "always"],

    "strict": 0,

    "init-declarations": 0,
    "no-catch-shadow": 2,
    "no-delete-var": 2,
    // "no-label-var": 2,
    "no-restricted-globals": 1,
    "no-shadow": 0,
    "no-shadow-restricted-names": 2,
    "no-undef": 1,
    "no-undef-init": 2,
    "no-undefined": 2,
    "no-unused-vars": 1,
    "no-use-before-define": [2, {
      "functions": false,
      "classes": false
    }],

    "camelcase": [2, {
      "properties": "never"
    }],
    "comma-spacing": [1, {
      "before": false,
      "after": true
    }],
    "brace-style": [1, "1tbs", {
      "allowSingleLine": true
    }],
    "array-bracket-spacing": [2, "never"],

    "no-const-assign": 2,

    "semi": [1, "always"],
    "quotes": [2, "single", {
      "avoidEscape": true,
      "allowTemplateLiterals": true
    }],
    "space-before-blocks": 2
  }
}